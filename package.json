{
  "name": "inc_webapp",
  "version": "0.0.1",
  "description": "LUTE's Web Audio-based performance tool for Terry Riley's \"In C\"",
  "main": "index.js",
  "scripts": {
    "build-js": "browserify -v js/main.js -o bundle.js",
    "build-sass": "node-sass styles/main.scss style.css",
    "build": "npm run build-js && npm run build-sass",
    "watch-js": "watchify -v --debug js/main.js -o bundle.js",
    "watch-sass": "node-sass -w --source-map-embed styles/main.scss style.css",
    "watch": "npm run watch-js & npm run watch-sass",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loyola-university-tech-ensemble/InC_WebApp.git"
  },
  "keywords": [
    "Terry Riley",
    "In C",
    "web audio",
    "laptop ensemble",
    "laptop orchestra",
    "technology ensemble"
  ],
  "author": "Loyola University Technology Ensemble",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loyola-university-tech-ensemble/InC_WebApp/issues"
  },
  "homepage": "https://github.com/loyola-university-tech-ensemble/InC_WebApp#readme",
  "dependencies": {
    "feather-icons": "^4.5.0",
    "tone": "^0.11.11"
  },
  "devDependencies": {
    "browserify": "^14.5.0",
    "node-sass": "^4.7.2",
    "watchify": "^3.9.0"
  }
}
